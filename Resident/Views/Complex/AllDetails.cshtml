@model IEnumerable<Resident.Models.ResidentialComplex>

@{
    ViewBag.Title = "AllDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ResComCat.Cat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameAlt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Abbr)
        </th>        
        <th>
            @Html.DisplayNameFor(model => model.FoundedYr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Founded)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ResComCat.Cat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameAlt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Abbr)
        </td>
        <td>
            @*@Html.DisplayFor(modelItem => item.Founded.Value.ToShortDateString())*@
            @Convert.ToString(string.Format("{0:dd/MMM/yyyy}", item.Founded))
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FoundedYr)
        </td>
        
    </tr>
}

</table>
